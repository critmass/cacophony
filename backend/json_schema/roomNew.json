{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "required":[
        "name",
        "server_id",
        "type"
    ],
    "properties": {
        "name":{
            "type": "string",
            "minLength": 1,
            "maxLength": 32
        },
        "server_id": {
            "type": "integer"
        },
        "type": {
            "type": "string",
            "minLength": 4,
            "maxLength": 10
        }
    }
}